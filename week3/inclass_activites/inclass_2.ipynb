    "def Factorial(n):\n",
    "    n=int(n)\n",
    "    result = int(1)\n",
    "    if n<0:\n",
    "        print(\"error\")\n",
    "    elif n==0 or n==1:\n",
    "        return result\n",
    "    else:\n",
    "        for i in range(1,n+1,1):\n",
    "            result = result * i\n",
    "            print(result)\n",
    "        return result\n",
    "    \n",
    "print(Factorial(10))"
   ]
  }
